---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Kingdom Building Ministries</title>
  <link href="http://www.kbm.org/"/>
  <link type="application/atom+xml" rel="self" href="http://kbm.org/feed.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://kbm.com/</id>
  <author>
    <name>Your Name</name>
    <email>you@example.com</email>
  </author>

  {% for post in site.categories.blog %}
  <entry>
    <id>http://kbm.org{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://kbm.com{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <published>{{ post.date | date_to_xmlschema }}</published>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Your Name</name>
      <uri>http://kbm.com/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>